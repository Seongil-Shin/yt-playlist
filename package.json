{
  "name": "yt-playlist",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "clsx": "^1.2.1",
    "npm-watch": "^0.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.7",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "public"
      ],
      "extensions": "*",
      "runOnChangeOnly": true
    }
  },
  "scripts": {
    "start": "npm-watch",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build && webpack --config webpack.config.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chrome": "^0.0.217",
    "customize-cra": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.2.7",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
